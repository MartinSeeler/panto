{
  "name": "panto",
  "version": "0.0.16",
  "description": "PantoJS is an ambitious file-transforming task runner",
  "main": "index.js",
  "scripts": {
    "test": "eslint src test && PANTO_LOG_LEVEL=error ./node_modules/.bin/mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "precommit": "test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pantojs/panto.git"
  },
  "keywords": [
    "panto",
    "build",
    "task",
    "runner",
    "tool",
    "transform"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "yanni4night@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pantojs/panto/issues"
  },
  "homepage": "https://github.com/pantojs/panto#readme",
  "dependencies": {
    "chokidar": "^1.5.2",
    "define-frozen-property": "^0.1.0",
    "glob": "^7.0.5",
    "lodash": "^4.13.1",
    "minimatch": "^3.0.2",
    "panto-dependency-map": "^0.1.0",
    "panto-file-utils": "^0.1.0",
    "panto-logger": "^0.2.1",
    "panto-options": "^0.1.0",
    "panto-stream": "^0.5.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "panto-transformer": "^0.2.2",
    "pre-commit": "^1.1.3"
  }
}
